<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".appSettingsFragment">

    <ImageView
        android:id="@+id/sign_out_iv"
        android:layout_width="134dp"
        android:layout_height="133dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="124dp"
        android:layout_marginEnd="51dp"
        app:layout_constraintEnd_toStartOf="@+id/brightnessIv"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/sign_out"
        tools:src="@tools:sample/avatars"
        tools:srcCompat="@mipmap/sign_out" />

    <ImageView
        android:id="@+id/brightnessIv"
        android:layout_width="128dp"
        android:layout_height="140dp"
        android:layout_marginEnd="58dp"
        app:layout_constraintBottom_toBottomOf="@+id/sign_out_iv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/sign_out_iv"
        app:layout_constraintTop_toTopOf="@+id/sign_out_iv"
        app:srcCompat="@mipmap/screenbrightness_foreground"
        tools:src="@tools:sample/avatars" />

</androidx.constraintlayout.widget.ConstraintLayout>