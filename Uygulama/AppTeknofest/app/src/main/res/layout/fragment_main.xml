<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainFragment">

    <ImageView
        android:id="@+id/cameraIv"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="149dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="23dp"
        app:layout_constraintBottom_toTopOf="@+id/appSettingsIv"
        app:layout_constraintEnd_toStartOf="@+id/diagnosticIv"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/cameraicon" />

    <ImageView
        android:id="@+id/diagnosticIv"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="149dp"
        android:layout_marginEnd="47dp"
        android:layout_marginBottom="31dp"
        app:layout_constraintBottom_toTopOf="@+id/deviceSettingsIv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cameraIv"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/diagnostic_round" />

    <ImageView
        android:id="@+id/deviceSettingsIv"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="47dp"
        android:layout_marginBottom="227dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/appSettingsIv"
        app:layout_constraintTop_toBottomOf="@+id/diagnosticIv"
        app:srcCompat="@mipmap/appsetings" />

    <ImageView
        android:id="@+id/appSettingsIv"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="45dp"
        android:layout_marginEnd="43dp"
        android:layout_marginBottom="226dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/deviceSettingsIv"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cameraIv"
        app:srcCompat="@mipmap/devicesetting_foreground"
        tools:src="@mipmap/devicesetting_foreground" />

</androidx.constraintlayout.widget.ConstraintLayout>